@model Project2WooxTravel.Entities.Admin

@{
    Layout = null;
}

<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<form method="post">
    <div class="container">
        <br /><br />
        <div class="alert alert-primary"><h3>Yeni Kullanıcı Kayıt Sayfası</h3></div>
        <label>Adnız</label>
        @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
        <br />
        <label>Soyadnız</label>
        @Html.TextBoxFor(x => x.Surname, new { @class = "form-control" })
        <br />
        <label>Email Adresiniz</label>
        @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
        <br />
        <label>Kullanıcı Adnız</label>
        @Html.TextBoxFor(x => x.UserName, new { @class = "form-control" })
        <br />
        <label>Şifreniz</label>
        @Html.PasswordFor(x => x.Password, new { @class = "form-control" })
        <br />
        <button class="btn btn-primary">Kaydet</button>
    </div>
</form>